    "babel-loader": "5.3.2",
    "css-loader": "0.23.1",
    "eslint-plugin-react": "^3.16.1",
    "express-http-proxy": "0.6.0",
    "html-webpack-plugin": "1.6.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "0.8.2",
