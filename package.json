{
  "name": "synfrastructure",
  "version": "2.0.2",
  "description": "Lightweight Sass utility library inspired by Foundation and Bourbon",
  "author": "Synapse Studios",
  "main": "index.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
