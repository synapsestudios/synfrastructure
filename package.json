{
  "name"           : "synfrastructure",
  "version"        : "3.0.0",
  "description"    : "Lightweight Sass utility library inspired by Foundation and Bourbon",
  "author"         : "Synapse Studios",
  "main"           : "index.js",
  "readmeFilename" : "README.md",
  "repository" : {
    "type" : "git",
    "url"  : "git://github.com/synapsestudios/synfrastructure.git"
  },
  "scripts" : {
    "demo"  : "rm -rf ./demo-build/* && ./node_modules/webpack/bin/webpack.js --config webpack.config.demo.js && node ./demo-server.js",
    "build" : "rm -f ./dist/*  && ./node_modules/babel/bin/babel/index.js src --loose --out-dir dist"
  },
  "dependencies": {
    "autoprefixer-core"   : "5.2.0",
    "babel"               : "5.6.4",
    "babel-loader"        : "5.1.4",
    "classnames"          : "2.1.2",
    "css-loader"          : "0.14.5",
    "express-http-proxy"  : "0.6.0",
    "html-webpack-plugin" : "1.5.2",
    "postcss-loader"      : "0.5.0",
    "react"               : "0.13.3",
    "react-hot-loader"    : "1.2.7",
    "react-tools"         : "0.13.3",
    "request"             : "2.58.0",
    "sass-loader"         : "1.0.2",
    "style-loader"        : "0.12.3",
    "lodash"              : "3.9.3",
    "webpack"             : "1.9.11",
    "webpack-dev-server"  : "1.9.0"
  }
}
